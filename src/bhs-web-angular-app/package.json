{
  "name": "bhs-web-angular-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ng serve",
    "start-proxy": "node scripts/aspnetcore-https.mjs && run-script-os",
    "start-proxy:windows": "ng serve --configuration proxy --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start-proxy:default": "ng serve --configuration proxy --ssl-cert \"$HOME/.aspnet/https/$npm_package_name.pem\" --ssl-key \"$HOME/.aspnet/https/$npm_package_name.key\"",
    "build": "ng build",
    "build-release": "ng build --configuration release",
    "transform-release": "node scripts/transform-release.mjs",
    "lint": "ng lint",
    "test": "ng test",
    "test-ci": "ng test --configuration ci",
    "clean": "ng cache clean"
  },
  "dependencies": {
    "@angular/animations": "21.0.6",
    "@angular/common": "21.0.6",
    "@angular/compiler": "21.0.7",
    "@angular/core": "21.0.6",
    "@angular/forms": "21.0.6",
    "@angular/platform-browser": "21.0.6",
    "@angular/router": "21.0.6",
    "@auth0/auth0-angular": "^2.4.0",
    "@microsoft/applicationinsights-analytics-js": "3.3.10",
    "@microsoft/applicationinsights-angularplugin-js": "^15.3.8",
    "@microsoft/applicationinsights-web": "^3.3.10",
    "@standard-schema/spec": "^1.1.0",
    "bootstrap-icons": "1.13.1",
    "jwt-decode": "^4.0.0",
    "marked": "^15.0.12",
    "ngx-toastr": "^19.1.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "21.0.4",
    "@angular/compiler-cli": "21.0.6",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/eslint-plugin": "^1.6.5",
    "angular-eslint": "21.1.0",
    "daisyui": "^5.5.14",
    "eslint": "^9.39.2",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-better-tailwindcss": "4.0.0-rc.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-rxjs-angular-x": "^0.1.1",
    "eslint-plugin-rxjs-x": "~0.8.4",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "merge-partially": "^2.0.2",
    "postcss": "^8.5.6",
    "run-script-os": "^1.1.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16"
  }
}
