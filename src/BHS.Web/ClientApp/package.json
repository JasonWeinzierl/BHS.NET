    "eslint-plugin-rxjs": "^5.0.1",
    "jasmine-core": "^4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "merge-partially": "^2.0.2",
