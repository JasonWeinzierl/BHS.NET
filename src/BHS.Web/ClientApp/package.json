  "scripts": {
    "start": "ng serve --open",
    "build": "run-s lint build:app",
    "build:app": "ng build",
    "build:app:release": "ng build --configuration release",
    "transform:app:release": "npx ts-node ./scripts/transform-app-version.ts",
    "watch": "ng build --watch --configuration debug",
