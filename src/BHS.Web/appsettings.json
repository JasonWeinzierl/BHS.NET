{
  "ConnectionStrings": {
    "bhs": "Server=localhost;Database=bhs;Integrated Security=True;Connection Timeout=30;TrustServerCertificate=True"
  },
  "SendGridClientOptions": {
    "ApiKey": "null"
  },
  "ContactUsOptions": {
    "FromAddress": "contact@beltonhistoricalsociety.org",
    "FromName": "Belton Historical Society",
    "ToAddresses": [
      "weinzierljason@gmail.com"
    ]
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext" ]
  },

  "AllowedHosts": "*"
}
