{
  "version": "0.2.0",
  "configurations": [{
      "name": "Launch Chrome against localhost",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/src/bhs-web-angular-app"
    },
    {
      "name": "Launch Firefox against localhost",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/src/bhs-web-angular-app"
    }
  ]
}
